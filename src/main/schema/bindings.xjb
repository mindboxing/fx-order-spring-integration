<?xml version="1.0" encoding="UTF-8"?>
<jaxb:bindings
        xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
        xmlns:xs="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:xsd='http://www.w3.org/2001/XMLSchema'
        xs:schemaLocation="http://java.sun.com/xml/ns/jaxb http://www.oracle.com/webfolder/technetwork/jsc/xml/ns/jaxb/bindingschema_2_0.xsd"
        jaxb:version="2.0"
>

    <jaxb:globalBindings>
        <jaxb:javaType name="java.time.ZonedDateTime"  xmlType="xsd:dateTime" hasNsContext="false"
                       parseMethod="jt.skunkworks.dataflow.message.converter.UtcDateTimeConverter.parse"
                       printMethod="jt.skunkworks.dataflow.message.converter.UtcDateTimeConverter.print"/>
    </jaxb:globalBindings>
</jaxb:bindings>